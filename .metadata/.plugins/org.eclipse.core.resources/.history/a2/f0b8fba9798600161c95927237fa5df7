package practice.test2A;

import java.util.List;

public class Test2A {

	private Test2A(){
		
	}
	
	public static char first(String s){
		return s.charAt(0);
	}
	
	public static char second(String s) throws IllegalArgumentException{
		try {
			return s.charAt(1);
		}
		catch(Exception e){
			throw new IllegalArgumentException();
		}
	}
	
}
